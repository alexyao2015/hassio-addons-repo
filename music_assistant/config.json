{
  "name": "Music Assistant",
  "version": "0.0.60",
  "description": "Media library manager for (streaming) media.",
  "url": "https://music-assistant.github.io",
  "slug": "music_assistant",
  "startup": "application",
  "boot": "auto",
  "arch": ["amd64", "armhf", "armv7", "aarch64", "i386"],
  "map": ["share:rw","ssl","config:rw"],
  "webui": "http://[HOST]:[PORT:8095]",
  "tmpfs": "size=64m",
  "host_network": true,
  "environment": {},
  "homeassistant_api": true,
  "ingress": true,
  "ingress_port": 8095,
  "panel_icon": "mdi:library-music",
  "options": {
    "debug_messages": false,
    "use_nightly": false
  },
  "schema": {
    "debug_messages": "bool",
    "use_nightly": "bool"
  }
}
